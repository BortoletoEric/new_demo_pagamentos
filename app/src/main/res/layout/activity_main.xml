<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".acitivities.MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="73dp"
            android:layout_height="32dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_gertec_logo_white" />
    </androidx.appcompat.widget.Toolbar>


    <com.airbnb.lottie.LottieAnimationView
    android:id="@+id/imageViewLedAzul"
    android:layout_width="40dp"
    android:layout_height="40dp"
    android:visibility="gone"
    app:lottie_loop="true"
    app:lottie_speed="1"
    app:lottie_autoPlay="true"
    android:layout_margin="20dp"
    android:translationZ="10dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:lottie_rawRes="@raw/blueledlottie" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/imageViewLedLaranja"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:lottie_loop="true"
        app:lottie_speed="1"
        app:lottie_autoPlay="true"
        android:layout_margin="20dp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_rawRes="@raw/orangeledlottie" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/imageViewLedVerde"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:lottie_loop="true"
        app:lottie_speed="1"
        app:lottie_autoPlay="true"
        android:layout_margin="20dp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_rawRes="@raw/greenledlottie" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/imageViewLedVermelho"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:lottie_loop="true"
        app:lottie_speed="1"
        app:lottie_autoPlay="true"
        android:layout_margin="20dp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_rawRes="@raw/redledlottie" />

    <fragment
        android:id="@+id/navHostFragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navGraph="@navigation/nav_graph" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_percent="0.65"
        app:layout_constraintStart_toStartOf="parent" />


